{
    "name": "AquilaX",
    "version": "0.4.1",
    "description": "AquilaX smart bookmarking",
    "omnibox": { "keyword" : "!!" },
    "permissions": ["storage", "declarativeContent", "activeTab", "tabs", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "assets/img/logo.png"
        },
        "icons": {
            "128": "assets/img/logo.png"
        }
    },
    "externally_connectable": {
        "matches": ["*://*.aquila.network/*"]
    },
    "manifest_version": 3
}