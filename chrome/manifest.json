{
    "name": "Aquila X",
    "version": "0.3.0",
    "description": "Aquila X page indexer",
    "omnibox": { "keyword" : "!!" },
    "permissions": ["storage", "declarativeContent", "activeTab", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/logo.png"
        },
        "icons": {
            "128": "images/logo.png"
        }
    },
    "externally_connectable": {
        "matches": ["*://*.aquila.network/*"]
    },
    "manifest_version": 2
}